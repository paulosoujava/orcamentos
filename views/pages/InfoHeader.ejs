
<div class="row ">
  <div class="col s12 m12">
    <div class="col s3 m3"></div>
    <div class="col s6 m6">
      <div class="row center">
        <a href="/" class="btn-floating btn-small waves-effect waves-light black left"><i class="material-icons">home</i></a>
        <div class="card">
            <div class="card-content">
              <br />
              <p class="left flow-text">Empresa</p>
            <div class="row">
                 <form class="col s12 ">
                  <br /> <br />
                  <div class="row">
                    <div class="input-field col s12">
                      <i class="material-icons prefix">home</i>
                      <input placeholder="nome" id="nome" type="text" value="<%= data.nome  %>" style="padding-left: 10px;">
                      <label for="nome">Nome</label>
                    </div>
                    <div class="input-field col s6">
                      <i class="material-icons prefix">map</i>
                      <input id="cnpj" type="text"  value="<%= data.cnpj %>" style="padding-left: 10px;" >
                      <label for="cnpj">CNPJ</label>
                    </div>
                    <div class="input-field col s6">
                      <i class="material-icons prefix">email</i>
                      <input id="email" type="email" value="<%= data.email %>" style="padding-left: 10px;">
                      <label for="email">Email</label>
                    </div>
                    <div class="input-field col s12">
                      <i class="material-icons prefix">phone</i>
                      <input id="telefone" type="text" value="<%= data.telefone %>" style="padding-left: 10px;">
                      <label for="telefone">Telefone</label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s12">
                      <i class="material-icons prefix">map</i>
                      <textarea id="endereco"  class="materialize-textarea" style="padding-left: 10px;"><%= data.endereco  %></textarea>
                      <label for="endereco">Endere√ßo</label>
                    </div>
                  </div>
                  <button id="createHeader" type="submit" class="btn  right"><i class="material-icons">save</i></button>
                </form>
              </div>
            </div>
          </div>
        <br />
      </div>
    </div>

    <div class="col s4 m4"></div>
  </div>
 
</div>
