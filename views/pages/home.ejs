<div class="section no-pad-bot" id="index-banner">
    <div class="container">
      <blockquote>
      <p class="header  black-text flow-text" style="margin-bottom: -10px; ">Último Número 015 </p>
      &nbsp;&nbsp;<small >Total de clientes <%= clients.length %></small>
    </blockquote>
    </div>
  </div>
  <div class="container">
    <div class="section">

      <!--   Icon Section   -->
      <div class="row">
        <% clients.forEach(function(client){ %>
        <div class="col s12 m6">
          <div class="card  darken-1">
            <div class="card-content black-text">
              <span class="card-title"><%= client.nome %></span>
              <ul class="collection with-header">
                <li class="collection-header"><h4>Infos:</h4></li>
                <li class="collection-item"><div>Email: <%= client.email %><a href="#!" class="secondary-content"><i class="material-icons">email</i></a></div></li>
                <li class="collection-item"><div>Telefone <%= client.telefone %><a href="#!" class="secondary-content"><i class="material-icons">phone</i></a></div></li>
                <li class="collection-item"><div>Cidade: <%= client.cidade %> - <%= client.uf %><a href="#!" class="secondary-content"><i class="material-icons">place</i></a></div></li>
                <li class="collection-item"><div>CPF/CNPJ: <%= client.cpfCnpj %><a href="#!" class="secondary-content"><i class="material-icons">account_box</i></a></div></li>
                <li class="collection-item"><div>Total de OrcPed <b><%= client.orcPeds.length %></b><a href="#!" class="secondary-content"><i class="material-icons">filter_9_plus</i></a></div></li>
              </ul>
            </div>
            <div class="card-action">
              <div class="row">
                <div class="col s12 m12 center">
                  <div class="col s4 m4 ">
                    <a  onClick=(deleteClient('<%= client.idGeneric %>')) class="btn-floating hoverable btn-large waves-effect waves-light red"><i class="material-icons">delete</i></a>
                  </div>
                  <div class="col s4 m4">
                    <a href="/pfpj?email=<%=client.email%>" class="btn-floating hoverable btn-large waves-effect waves-light green"><i class="material-icons">edit</i></a>
                  </div>
                  <div class="col s4 m4">
                    <a  href="/listar?id=<%= client.idGeneric %>" class="btn-floating hoverable btn-large waves-effect waves-light teal"><i class="material-icons">list</i></a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

        <% }) %>
      </div>

    </div>
    <br><br>
  </div>
