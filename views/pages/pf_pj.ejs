
<div class="row ">
  <div class="col s12 m12">
    <div class="col s2 m2"></div>
    <div class="col s8 m8">
      <div class="row center">
        <a href="/" class="btn-floating btn-small waves-effect waves-light black left"><i class="material-icons">home</i></a>
        <div class="card">
            <div class="card-content">
              <br />
              <p class="left flow-text"><%= data.title %></p>
              <div class="row">
                 <form class="col s12 ">
                  <br /> <br />
                  <div class="row">
                    <div class="input-field col s12">
                      <i class="material-icons prefix">account_circle</i>
                      <input  id="nome" type="text"  value="<%= data.client.nome %>">
                      <label for="nome">Nome</label>
                    </div>
                    <div class="input-field col s6">
                      <i class="material-icons prefix">email</i>
                      <input id="email" type="email" value="<%= data.client.email %>">
                      <label for="email">Email</label>
                    </div>
                    <div class="input-field col s6">
                      <i class="material-icons prefix">account_circle</i>
                      <input id="cpfCnpj" type="text" value="<%= data.client.cpfCnpj %>">
                      <label for="cpfCnpj">CNPJ ou CPF</label>
                    </div>
                    <div class="input-field col s6">
                      <i class="material-icons prefix">phone</i>
                      <input id="telefone" type="text" value="<%= data.client.telefone %>">
                      <label for="telefone">Telefone</label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s8">
                      <i class="material-icons prefix">place</i>
                      <input id="cidade" type="text" value="<%= data.client.cidade %>">
                      <label for="cidade">Cidade</label>
                    </div>
                    <div class="input-field col s4">
                      <i class="material-icons prefix">place</i>
                      <input id="uf" type="text" value="<%= data.client.uf %>">
                      <label for="uf">UF</label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s12">
                      <i class="material-icons prefix">map</i>
                      <textarea id="endereco"  class="materialize-textarea"><%= data.client.endereco %></textarea>
                      <label for="endereco">Endere√ßo</label>
                    </div>
                  </div>
                  <div class="row">
                    <div class="input-field col s12">
                      <i class="material-icons prefix">forum</i>
                      <input id="obs" type="text" value="<%= data.client.obs %>">
                      <label for="obs">Obs</label>
                    </div>
                  </div>
                  <input type="hidden" id="idClient" name="id" value="<%= data.client.idGeneric %>" />
                  <button id="createOrUpdateBtn" type="submit" class="btn  right"><i class="material-icons">save</i></button>
                </form>
              </div>
            </div>
          </div>
        <br />
      </div>
    </div>

    <div class="col s4 m4"></div>
  </div>
 
</div>
